@page
@model AssetManager.Pages.AccountListModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers


<table class="table">
    <thead>
    <tr>
        <th scope="col">Name</th>
        <th scope="col">Code</th>
        <th scope="col">Type</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var account in Model.Accounts)
    {
    <tr>
        <td>@account.Name</td>
        <td>@account.Code</td>
        <td>@account.Type</td>
    </tr>
    }
    </tbody>
</table>